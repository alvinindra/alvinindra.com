{
  "name": "alvinindra",
  "version": "0.0.1",
  "description": "A simple CLI wrapper for adding components from alvinindra.com component registry using shadcn.",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "start:dev": "node dist/index.js",
    "publish:release": "pnpm build && pnpm publish --access public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "alvinindra",
    "cli",
    "shadcn"
  ],
  "author": "Alvin Indra Pratama <alvinindrap@gmail.com>",
  "license": "MIT",
  "homepage": "https://alvinindra.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alvinindra/alvinindra.com.git",
    "directory": "packages/ncdai"
  },
  "funding": "https://github.com/sponsors/alvinindra",
  "bugs": {
    "url": "https://github.com/alvinindra/alvinindra.com/issues"
  },
  "devDependencies": {
    "tsup": "^8.5.0"
  },
  "packageManager": "pnpm@10.10.0"
}
